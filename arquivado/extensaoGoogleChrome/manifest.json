{
    "manifest_version": 3,
    "name": "Captura e armazenamento de informações",
    "version": "1.0",
    "description": "Extensão para capturar e armazenar informações de uma página da web.",
    "permissions": ["activeTab", "storage", "tabs"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
  